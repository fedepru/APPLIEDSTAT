file_path <- "C:\\Users\\Ascolani\\Desktop\\PROJECTRAULBOA\\dataraw.csv"
data <- read.csv(file_path, header = TRUE, stringsAsFactors = FALSE, sep = ",", skip = 0)
# Data transformation
data_wide <- data %>%
group_by(platformId, time, latitude, longitude, parameter) %>%
summarise(value = mean(value, na.rm = TRUE), .groups = "drop") %>%
pivot_wider(names_from = parameter, values_from = value)
# Convert time to proper datetime format
data_wide <- data_wide %>%
mutate(
datetime = ymd_hms(time),  # Convert to date-time
date = as.integer(format(datetime, "%Y%m%d")),
time_numeric = as.numeric(datetime)  # Convert time to numeric for animation
) %>%
arrange(platformId, datetime)  # Ensure ordered data
data_wide$time_numeric <- as.numeric(as.POSIXct(data_wide$datetime))
data_wide %>%
group_by(platformId) %>%
summarise(count = n()) %>%
arrange(count)
test_buoy <- data_wide %>% filter(platformId == "C8E5")  # Change ID as needed
